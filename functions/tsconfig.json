{
    "compilerOptions": {
        "target": "ES2021",
        "lib": [
            "ES2021"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "types": [
            "@cloudflare/workers-types"
        ],
        "noEmit": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": [
        "functions/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}